<?xml version="1.0" encoding="utf-8"?>

<configuration>
    <!--Type of Connection Controller-->
    <cc_type>domain</cc_type>
    <!--Local port of own ConnectionRequest listener-->
    <cc_connection_request_listener_local_port>42821</cc_connection_request_listener_local_port>
    <!--Local port of own PeerCoordination listener-->
    <cc_connection_request_listener_local_port>42822</cc_connection_request_listener_local_port>
    <!--Remote port of second domain CC PeerCoordination listener-->
    <cc_peer_coordination_remote_port>43812</cc_peer_coordination_remote_port>
    <!--Localhost ip-->
    <server_address>127.0.0.1</server_address>

    <!--With this dict CC knows by subnetwork CC names by port-->
    <cc_name_by_port>
        
        <element>
            <port>3xx</port>
            <cc_name>CC_SN3</cc_name>
        </element>
       
    </cc_name_by_port>

    <!--With this dict CC knows remote port of CC:ConnectionRequest listener of its subnetwork CCs-->
    <connection_request_local_port_by_cc_name>
        
        <element>
            <cc_name>CC_SN3</cc_name>
            <connection_request_local_port>42931</connection_request_local_port>
        </element>
        
    </connection_request_local_port_by_cc_name>

</configuration>
